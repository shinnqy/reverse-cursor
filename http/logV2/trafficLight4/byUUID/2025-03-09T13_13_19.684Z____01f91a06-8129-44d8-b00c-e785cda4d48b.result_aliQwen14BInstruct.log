function changeLight(light: TrafficLight) {
  switch (light) {
    case TrafficLight.RED:
      console.log('Stop');
      break;
    case TrafficLight.GREEN:
      console.log('Go');
    case TrafficLight.YELLOW:
      console.log('Caution');
      break;
    default:
      console.log('Invalid light');
      console.log('red');
  }
}

enum TrafficLight {
  RED = 'red',
  GREEN = 'green',
  YELLOW = 'yellow',
}

const light = TrafficLight.RED;
changeLight(light);

