function changeLight(light: TrafficLight) {
  switch (light) {
    case TrafficLight.RED:
      console.log('red');
      break;
    case TrafficLight.GREEN:
      console.log('green');
      break;
    case TrafficLight.YELLOW:
      console.log('yellow');
      break;
    default:
      console.log('red');
  }
}

enum TrafficLight {
  RED = 'red',
  GREEN = 'green',
  YELLOW = 'yellow',
}

const light = TrafficLight.RED;
changeLight(light);

