        .fontSize(20)
        .fontWeight(FontWeight.Bold)
        .onClick(() => {
          router.pushUrl({
            url: "pages/SecondPage",
            params: {
              message: "Hello from Second Page",
            },
          });
        })
        .width(200)
        .height(100)

    }
  }
}

